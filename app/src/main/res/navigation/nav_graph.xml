<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">
    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.webantpractice.screens.HomeFragment"
        android:label="HomeFragment" >
        <action
            android:id="@+id/action_homeFragment_to_selectedPhotoFragment2"
            app:destination="@id/selectedPhotoFragment2" />
    </fragment>
    <fragment
        android:id="@+id/setPhotoDataFragment"
        android:name="com.example.webantpractice.screens.SetPhotoDataFragment"
        android:label="SetPhotoDataFragment" />
    <fragment
        android:id="@+id/userProfileFragment"
        android:name="com.example.webantpractice.screens.UserProfileFragment"
        android:label="UserProfileFragment" >
        <action
            android:id="@+id/action_userProfileFragment_to_userProfileSettingsFragment2"
            app:destination="@id/userProfileSettingsFragment2" />
    </fragment>
    <fragment
        android:id="@+id/selectedPhotoFragment2"
        android:name="com.example.webantpractice.screens.SelectedPhotoFragment"
        android:label="SelectedPhotoFragment" >
        <action
            android:id="@+id/action_selectedPhotoFragment2_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/userProfileSettingsFragment2"
        android:name="com.example.webantpractice.screens.UserProfileSettingsFragment"
        android:label="UserProfileSettingsFragment" >
        <action
            android:id="@+id/action_userProfileSettingsFragment2_to_userProfileFragment"
            app:destination="@id/userProfileFragment" />
    </fragment>
</navigation>